{
  "name": "blumilksoftware/internships",
  "description": "Internship programme website made by interns themselves.",
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Internships\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "ext-gd": "*",
    "ext-pdo": "*",
    "ext-redis": "*",
    "fruitcake/laravel-cors": "^3.0",
    "inertiajs/inertia-laravel": "^0.6.3",
    "intervention/image": "^2.7",
    "laravel/framework": "^9.43",
    "laravel/sanctum": "^3.0",
    "laravel/socialite": "^5.5",
    "laravel/telescope": "^4.9",
    "laravel/tinker": "^2.7.3",
    "laravolt/avatar": "^4.1",
    "spatie/laravel-data": "^2.0",
    "spatie/laravel-model-states": "^2.1",
    "symfony/yaml": "^6.2",
    "tightenco/ziggy": "^1.5"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.7",
    "blumilksoftware/codestyle": "^1.9",
    "fakerphp/faker": "^1.20",
    "laravel/dusk": "^7.1",
    "mockery/mockery": "^1.5",
    "nunomaduro/collision": "^6.3",
    "phpunit/phpunit": "^9.5",
    "spatie/laravel-ignition": "^1.2"
  },
  "scripts": {
    "cs": "vendor/bin/php-cs-fixer fix --dry-run --diff --config codestyle.php",
    "csf": "vendor/bin/php-cs-fixer fix --diff --config codestyle.php",
    "test": "@php artisan test",
    "fresh": "@php artisan migrate:fresh",
    "fresh:demo": [
      "composer fresh",
      "@php artisan db:seed --class=DemoSeeder"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/telescope",
        "spatie/laravel-slack-slash-command"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

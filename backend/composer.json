{
  "name": "blumilksoftware/internships",
  "description": "Internship programme website made by interns themselves.",
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Internships\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "ext-pdo": "*",
    "ext-redis": "*",
    "azuyalabs/yasumi": "^2.4",
    "barryvdh/laravel-dompdf": "^2.0",
    "fruitcake/laravel-cors": "^3.0",
    "geocoder-php/common-http": "^4.4.0",
    "geocoder-php/mapbox-provider": "^1.3",
    "guzzlehttp/guzzle": "^7.0.1",
    "inertiajs/inertia-laravel": "^0.6.2",
    "jessarcher/laravel-castable-data-transfer-object": "^2.2",
    "laravel/framework": "^9.7",
    "laravel/sanctum": "^3.0",
    "laravel/socialite": "^5.2",
    "laravel/telescope": "^4.6",
    "laravel/tinker": "^2.5",
    "maatwebsite/excel": "^3.1",
    "php-di/php-di": "^6.3.4",
    "php-http/guzzle7-adapter": "^1.0",
    "phpoffice/phpword": "^0.18.3",
    "spatie/data-transfer-object": "^3.8",
    "spatie/laravel-model-states": "^2.1",
    "vlucas/phpdotenv": "^v5.3.0"
  },
  "require-dev": {
    "blumilksoftware/codestyle": "^1.3",
    "fakerphp/faker": "^1.19",
    "laravel/dusk": "^6.24",
    "mockery/mockery": "^1.5",
    "nunomaduro/collision": "^6.2",
    "phpunit/phpunit": "^9.5",
    "spatie/laravel-ignition": "^1.2"
  },
  "scripts": {
    "intern": [
      "@intern-help"
    ],
    "intern-help": [
      "sh -c 'cd static && php index.php --help --composer'"
    ],
    "intern-build": [
      "rm -rf ../public/api",
      "sh -c 'cd static && php index.php --build'"
    ],
    "intern-populate": [
      "sh -c 'cd static && php index.php --populate'"
    ],
    "intern-fetch": [
      "sh -c 'cd static && php index.php --fetch'"
    ],
    "intern-purge": [
      "rm -rf ../public/api"
    ],
    "test-backend": "vendor/bin/phpunit testing --filter Backend",
    "test-resources": "vendor/bin/phpunit testing --filter Integrity",
    "cs": "vendor/bin/php-cs-fixer fix --dry-run --diff --config codestyle.php",
    "csf": "vendor/bin/php-cs-fixer fix --diff --config codestyle.php",
    "test": "@php artisan test",
    "fresh": "@php artisan migrate:fresh",
    "fresh:demo": [
      "composer fresh",
      "@php artisan db:seed --class=DemoSeeder"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ]
  },
  "scripts-descriptions": {
    "intern-help": "Prints help for application."
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/telescope",
        "spatie/laravel-slack-slash-command"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
